<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <RootNamespace>Voidway_Bot</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Relauncher\**" />
    <EmbeddedResource Remove="Relauncher\**" />
    <None Remove="Relauncher\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DSharpPlus" Version="4.4.0-nightly-01291" />
    <PackageReference Include="DSharpPlus.Interactivity" Version="4.4.0-nightly-01291" />
    <PackageReference Include="DSharpPlus.SlashCommands" Version="4.4.0-nightly-01291" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="7.0.0" />
    <PackageReference Include="Samboy063.Tomlet" Version="5.1.2" />
    <PackageReference Include="System.Runtime" Version="4.3.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Modio">
      <HintPath>modio\Modio.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
